INSERT INTO `excuses` VALUES (38,'min inneboende pingvin Larry har gjort en iskana genom hela lägenheten! Titta inte sådär på mig. Det är inte mitt fel att Larry vill känna sig lite hemmastadd ibland.',0);
INSERT INTO `excuses` VALUES (39,'jag har hittat världens fräckaste skalbagge. Jag tror inte någon har sett en sådan förut. En helt ny art! Öh, vart var det jag la den nu igen…',0);
