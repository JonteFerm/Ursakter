INSERT INTO `excuses` VALUES (30,'jag var tvungen att säga hej då till ankorna som jag brukar hänga med, de ska söderut nu.',0);
INSERT INTO `excuses` VALUES (31,'jag har rymt med en guldfiskscirkus och ingen av deras klockor går rätt.
det blev en väldigt syndig kväll igår. Jag funderar fortfarande på hur jag ska kunna förklara glittret för min katt!',0);
